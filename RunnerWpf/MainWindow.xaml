<Window x:Class="RunnerWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RunnerWpf"
        mc:Ignorable="d"
        Title="Runner" Height="450" Width="800">
    <Canvas Name ="MyCanvas" Focusable="True" Background="LightBlue" KeyDown="MyCanvas_KeyDown" KeyUp="MyCanvas_KeyUp">
        <Rectangle Name ="Backround" Height="400" Width="1262" Fill="Orange"/>
        <Rectangle Name ="Backround2" Height="400" Width="1262" Fill="LightGreen" Canvas.Left="1262"/>
        <Rectangle Name ="Ground" Height="32" Width="800" Fill="Black" Canvas.Top="390" Canvas.Left="0"/>
        <Rectangle Name ="Player" Height="99" Width="67" Fill="Brown" Canvas.Left="100" Canvas.Top="150"/>
        <Label Name="ScoreText" Content="Score : 0" FontSize="20" FontWeight="Bold" />
        <Rectangle Name ="obstale" Height="180" Width="50" Fill="Red" Canvas.Left="700" Canvas.Top="320"/>

    </Canvas>
</Window>
